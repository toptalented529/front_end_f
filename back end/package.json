{
  "name": "server",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node ./src/index.ts",
    "server": "nodemon --exec sucrase-node src/index.ts ",
    "start:dev": "yarn docker:down && yarn docker:up && yarn server",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:destroy": "docker-compose down --volumes --remove-orphans",
    "migrate:up": "node-pg-migrate -m ./infra/migrations up",
    "migrate:down": "node-pg-migrate -m ./infra/migrations down",
    "migrate:redo": "DATABASE_URL=postgresql://postgres:postgres@localhost:5433/auth?schema=public node-pg-migrate -m ./infra/migrations redo",
    "migrate:create": "node-pg-migrate -m ./infra/migrations create",
    "prettier": "prettier 'src/**/*.{js,ts,md,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,ts,md,json}'"
  },
  "dependencies": {
    "@fastify/accepts": "^4.1.0",
    "@fastify/cors": "^8.1.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/jwt": "^6.3.2",
    "@fastify/multipart": "^7.4.0",
    "@fastify/request-context": "^4.0.0",
    "@gmetrixr/gdash": "^1.3.18",
    "@gmetrixr/rjson": "^1.1.42",
    "aws-sdk": "^2.1313.0",
    "axios": "^1.3.2",
    "brandi": "^4.0.2",
    "dotenv": "^16.0.3",
    "fastify": "^4.5.2",
    "fastify-axios": "^1.2.6",
    "fastify-file-upload": "^4.0.0",
    "fastify-multer": "^2.0.3",
    "fastify-multipart": "^5.4.0",
    "fluent-json-schema": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "node-pg-migrate": "^6.2.2",
    "nodemon": "^2.0.19",
    "pg": "^8.8.0",
    "pino-pretty": "^9.1.0",
    "ramda": "^0.28.0",
    "undici": "^5.10.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.13",
    "@types/pg": "^8.6.5",
    "@types/pino": "^7.0.5",
    "fastify-vhost": "^1.3.5",
    "prettier": "^2.7.1",
    "sucrase": "^3.25.0",
    "typescript": "*"
  }
}
